{
    "n_experiments": 10,
    
    "load_name": "exponential_fixed_cov",
    "normalize": false,
    "dtype": "float32",
    
    "hidden_dim": 5,
    "n_hidden_layers": 1,
    "hidden_dim_matrix": 100,
    "n_hidden_layers_matrix": 3,

    "batch_size_warm_start": 100,
    "num_epochs_warm_start": 100,
    
    "n_neighbors": 500,
    
    "batch_size_our_loss": 100,
    "warm_start_epochs_mat": 50,
    "num_epochs_our_loss": 400,
    
    "lr_warm_start": 0.01,
    "lr_model": 0.001,
    "lr_matrix": 0.001,
    "lr_q": 0.01,
    "use_lr_scheduler": true,
    "loss_strategy" : "log_volume",
    "use_epsilon": false,
    
    "batch_size_rectangles": 100,
    "num_epochs_rectangles": 200,
    "lr_rectangles": 0.0001,
    
    "keep_best": true,

    "prop_train": 0.7,
    "prop_stop": 0.1,
    "prop_calibration": 0.1,
    "prop_test": 0.1,

    "alpha": 0.1
